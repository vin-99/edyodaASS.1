    "var = int(input(\"Enter the Number where you want the series: \"))\n",
    "\n",
    "a = 0\n",
    "b = 1\n",
    "ind = 1\n",
    "while ind<=var:\n",
    "    c = a + b\n",
    "    a = b\n",
    "    b = c\n",
    "    ind+=1\n",
    "    print(a, end=\" , \")"
   ]
